# Godot editor config
root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true

[*.gd]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true

[*.cs]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true

[*.csproj]
indent_style = space
indent_size = 2
insert_final_newline = false
